  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^6.1.0",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.9",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "stripe": "^8.150.0"
  }
